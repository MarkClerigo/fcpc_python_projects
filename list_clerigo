# String

mga_bobo = ["Mark", "Ralph", "Jhanjan", "Lheo", "Neil"]
numero = [1, 2, 3, 4, 5, 6]
decimal = [0.1, 1.5, 2.3, 3.7, 4.2, 5.5]

for x in range(len(mga_bobo)):
        print(mga_bobo[x])

for y in range(len(numero)):
        print(numero[y])

for z in range(len(decimal)):
        print(decimal[z])

#counts_your_characters
print(len(mga_bobo))
print(len(numero))
print(len(decimal))
